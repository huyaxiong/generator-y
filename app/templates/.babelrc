{
  "presets": [
    "env"
  ],
  "plugins": [
    "transform-runtime",
    "syntax-dynamic-import",
    "transform-object-rest-spread",
    [
      "babel-plugin-root-import",
      {
        "paths": [
          {
            "rootPathSuffix": "./backend/",
            "rootPathPrefix": "~"
          },
          {
            "rootPathSuffix": "./src/",
            "rootPathPrefix": "@"
          }
        ]
      }
    ]
  ]
}